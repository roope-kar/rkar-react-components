{
    "extends": [
        "plugin:react/recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:jsx-a11y/recommended",
        "prettier",
        "prettier/@typescript-eslint"
    ],
    "plugins": [
        "react",
        "jest",
        "@typescript-eslint",
        "jsx-a11y"
    ],
    "settings": {
        "react": {
            "version": "detect"
        },
    },
    "env": {
        "browser": true,
        "es6": true,
        "amd": true,
        "mocha": true,
        "node": true
    },
    "globals": {
        "jest": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "experimantalObjectRestSpread": true
        }
    }
}
